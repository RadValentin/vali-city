  "bugs": {
    "url": "https://github.com/RadValentin/vali-city/issues"
  },
  "homepage": "https://github.com/RadValentin/vali-city#readme",
  "devDependencies": {
    "marked": "^12.0.1",
    "gh-pages": "^6.3.0",
    "normalize.css": "^8.0.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-copy": "^1.0.2",
    "parcel-plugin-nuke-dist": "^1.0.0"
  },
  "staticPath": {
    "source": [
      "/old-site/",
      "/CNAME",
      "/robots.txt",
      "/sitemap.xml"
    ],
    "target": [
      "/dist/old-site/",
      "/dist/CNAME",
